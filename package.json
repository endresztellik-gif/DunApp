{
  "name": "dunapp-temp",
  "private": true,
  "version": "1.9.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:edge-functions": "deno test --allow-net --allow-env --allow-read supabase/functions/tests/*.test.ts",
    "test:all": "npm run test && npm run test:edge-functions",
    "deploy": "./scripts/deploy.sh",
    "deploy:preview": "./scripts/deploy.sh --preview",
    "deploy:skip-tests": "./scripts/deploy.sh --skip-tests",
    "supabase:setup": "./scripts/setup-supabase.sh",
    "supabase:new": "./scripts/setup-supabase.sh --new-project",
    "supabase:link": "./scripts/setup-supabase.sh --link",
    "supabase:update": "./scripts/setup-supabase.sh --update",
    "lighthouse": "lhci autorun",
    "lighthouse:mobile": "lighthouse http://localhost:4173 --preset=mobile --view",
    "lighthouse:desktop": "lighthouse http://localhost:4173 --preset=desktop --view",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.76.1",
    "@tanstack/react-query": "^5.90.5",
    "esri-leaflet": "^3.0.19",
    "framer-motion": "^12.23.24",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.548.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.3.0",
    "suncalc": "^1.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/suncalc": "^1.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.4",
    "@vitest/ui": "^4.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.4",
    "workbox-window": "^7.3.0"
  }
}
